{
    "builds": [
      {
        "src": "workapp/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/migrate.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/migrate",
        "dest": "api/migrate.py"
      },
      {
        "src": "/(.*)",
        "dest": "workapp/wsgi.py"
      }
    ]
  }